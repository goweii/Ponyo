<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:orientation="vertical">

    <CheckBox
        android:id="@+id/cb_auto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="自动随机打印" />

    <CheckBox
        android:id="@+id/cb_service1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="多进程服务1" />

    <CheckBox
        android:id="@+id/cb_service2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="多进程服务2" />

    <Button
        android:id="@+id/tv_print_10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="打印10个数"
        android:textAllCaps="false"
        android:textSize="12sp" />

    <Button
        android:id="@+id/tv_print_assert"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Assert"
        android:textAllCaps="false"
        android:textSize="12sp" />

    <Button
        android:id="@+id/tv_print_error"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Error"
        android:textAllCaps="false"
        android:textSize="12sp" />

    <Button
        android:id="@+id/tv_print_warn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Warn"
        android:textAllCaps="false"
        android:textSize="12sp" />

    <Button
        android:id="@+id/tv_print_info"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Info"
        android:textAllCaps="false"
        android:textSize="12sp" />

    <Button
        android:id="@+id/tv_print_debug"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Debug"
        android:textAllCaps="false"
        android:textSize="12sp" />

    <Button
        android:id="@+id/tv_print_visible"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Visible"
        android:textAllCaps="false"
        android:textSize="12sp" />

</LinearLayout>